{
  "version": 2,
  "builds": [
    {
      "src": "build.sh",
      "use": "@vercel/python",
      "config": {
        "command": "bash build.sh"
      }
    },
    {
      "src": "main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb"
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "main.py"
    }
  ],
  "env": {
    "PYTHON_VERSION": "3.13"
  }
}